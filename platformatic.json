{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/2.5.5.json",
  "entrypoint": "composer",
  "watch": true,
  "logger": {
    "level": "{PLT_SERVER_LOGGER_LEVEL}"
  },
  "server": {
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": "{PORT}"
  },
  "managementApi": "{PLT_MANAGEMENT_API}",
  "services": [{
    "id": "ai",
    "path": "services/ai",
    "useHttp": true
  }, {
    "id": "composer",
    "path": "services/composer"
  }] 
}
